pipeline{
  agent any
  
  environment{
    PATH = "/opt/maven3/bin:$PATH"
    
    }
    
    stages{
    
      stage("Git Checkout"){
        steps{
         git credentialsId: 'GitHub', url: 'https://github.com/Ajdammalapati/samplejavappforpractice.git'
          }
         }
        stage("Maven Build"){
          steps{
              sh "mvn clean package"
              sh "mvn target/*.war"
              }
             }
           
            }
        }  
